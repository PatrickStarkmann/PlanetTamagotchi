<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/hintergrund"
        android:layout_width="1008dp"
        android:layout_height="1604dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        android:background="@drawable/hintergrund" />

    <ImageView
        android:id="@+id/tamagotchi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        android:background="@drawable/tamagotchi" />

    <ImageView
        android:id="@+id/stern"
        android:layout_width="79dp"
        android:layout_height="125dp"
        android:background="@drawable/star"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="1123dp" />

    <ImageView
        android:id="@+id/minigame"
        android:layout_width="164dp"
        android:layout_height="103dp"
        android:background="@drawable/videospielkonsole"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="308dp"
        tools:layout_editor_absoluteY="1138dp" />

    <ImageView
        android:id="@+id/shop"
        android:layout_width="148dp"
        android:layout_height="130dp"
        android:background="@drawable/shop"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="620dp"
        tools:layout_editor_absoluteY="1118dp" />

</androidx.constraintlayout.widget.ConstraintLayout>